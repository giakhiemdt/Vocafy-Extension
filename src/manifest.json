{
  "manifest_version": 3,
  "name": "Vocafy Extension",
  "version": "0.1.0",
  "description": "Dev-mode Chrome extension scaffold.",
  "action": {
    "default_title": "Vocafy",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["storage", "identity"],
  "key": "",
  "oauth2": {
    "client_id": "__OAUTH_CLIENT_ID__",
    "scopes": ["openid", "email", "profile"]
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
